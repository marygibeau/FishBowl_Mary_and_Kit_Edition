<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/brightNavyBlue</item>
        <item name="colorPrimaryDark">@color/brightNavyBlue</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="mainbackground">
        <item name="android:background">@color/brightNavyBlue</item>
    </style>
    <style name="background">
        <item name="android:background">@color/paleCyan</item>
    </style>
    <style name="button">
        <item name="android:background">@color/livid</item>
        <item name="android:textColor">@color/darkImperialBlue</item>
    </style>
    <style name="title">
        <item name="android:textColor">@color/darkImperialBlue</item>
    </style>
</resources>
